{
  "name": "huntedhorse",
  "version": "1.0.0",
  "description": "Hunted Horse",
  "author": {
    "name": "Randy Uebel",
    "email": "randy.uebel@gmail.com"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "displayName": false,
          "preprocess": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "gsandf-react",
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ruebel/huntedhorse"
  },
  "devDependencies": {},
  "main": "docs/index.js",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rm -rf ./docs",
    "build": "react-scripts build",
    "postbuild": "mv ./build ./docs",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
